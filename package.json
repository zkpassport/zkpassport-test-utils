{
  "name": "@zkpassport/test-utils",
  "version": "0.1.1",
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "files": [
    "src/",
    "dist/",
    "tsconfig.json"
  ],
  "exports": {
    "./asn": {
      "import": "./dist/esm/asn.js",
      "types": "./dist/types/asn.d.ts"
    },
    "./test-helper": {
      "import": "./dist/esm/test-helper.js",
      "types": "./dist/types/test-helper.d.ts"
    },
    "./sod-generator": {
      "import": "./dist/esm/sod-generator.js",
      "types": "./dist/types/sod-generator.d.ts"
    },
    "./passport-generator": {
      "import": "./dist/esm/passport-generator.js",
      "types": "./dist/types/passport-generator.d.ts"
    },
    "./circuits": {
      "import": "./dist/esm/circuits.js",
      "types": "./dist/types/circuits.d.ts"
    },
    "./utils": {
      "import": "./dist/esm/utils.js",
      "types": "./dist/types/utils.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "prepare": "npm i --save-dev @types/node && npm run build:esm && npm run build:types",
    "build": "npm run build:esm && npm run build:types",
    "build:esm": "tsc --outDir dist/esm --module esnext",
    "build:cjs": "tsc --outDir dist/cjs --module commonjs",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types"
  },
  "devDependencies": {
    "@noir-lang/types": "1.0.0-beta.1",
    "@types/bun": "latest",
    "@types/lapo__asn1js": "^1.2.5",
    "@types/node": "^22.10.5",
    "@types/node-forge": "^1.3.11",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@aztec/bb.js": "0.67.0",
    "@lapo/asn1js": "^2.0.4",
    "@noble/ciphers": "^1.0.0",
    "@noble/curves": "^1.6.0",
    "@noble/hashes": "^1.5.0",
    "@noble/secp256k1": "^2.1.0",
    "@noir-lang/noir_js": "1.0.0-beta.1",
    "@noir-lang/noir_wasm": "1.0.0-beta.1",
    "@peculiar/asn1-cms": "^2.3.13",
    "@peculiar/asn1-ecc": "^2.3.14",
    "@peculiar/asn1-rsa": "^2.3.13",
    "@peculiar/asn1-schema": "^2.3.13",
    "@peculiar/asn1-x509": "^2.3.13",
    "@peculiar/x509": "^1.12.3",
    "@zkpassport/poseidon2": "^0.5.0",
    "@zkpassport/utils": "^0.1.7",
    "date-fns": "^4.1.0",
    "i18n-iso-countries": "^7.13.0",
    "node-forge": "^1.3.1"
  }
}
